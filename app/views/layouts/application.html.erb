<!DOCTYPE html>
<html>

	<head>
		<title>Ideas Matter - Eureka</title>
		<%= stylesheet_link_tag    "application", :media => "all" %>
		<%= javascript_include_tag "application" %>
		<%= csrf_meta_tags %>
	</head>
	<body>
		<%= render :partial => "layouts/header" %>
		<div class="container yield">
			<%= yield %>
			<% if user_signed_in? %>
				<%= render :partial => 'layouts/new_idea_popup'%>
			<% end %>
		</div>
	</body>

</html>
